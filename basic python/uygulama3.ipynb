   "source": [
    "metin = input()\n",
    "newMetin = \"\"\n",
    "for index, letter in enumerate(metin):\n",
    "    if index % 2 == 0:\n",
    "        newMetin += letter.upper()\n",
    "    else:\n",
    "        newMetin += letter.lower()\n",
    "    \n",
    "print(newMetin)"
   ]
